# Shopify API credentials
# Získate v Shopify Admin > Settings > Apps and sales channels > Develop apps
SHOPIFY_STORE_URL=your-store.myshopify.com
SHOPIFY_ACCESS_TOKEN=shpat_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# DeepSeek API key
API_KEY=your-deepseek-api-key

# Upstash Redis (Klema Earrings projekt)
# Tieto premenné sa automaticky vytvoria pri pripojení Upstash Storage na Vercel
klema_shopify_KV_REST_API_URL=https://your-redis-url.upstash.io
klema_shopify_KV_REST_API_TOKEN=your-upstash-token
klema_shopify_KV_REST_API_READ_ONLY_TOKEN=your-read-only-token
klema_shopify_KV_URL=redis://your-redis-url
klema_shopify_REDIS_URL=redis://your-redis-url
